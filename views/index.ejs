<!DOCTYPE html>
<html lang="en">
    <head>
        <title>pepchat</title>
        <meta charset="UTF-8"> 
        <link href="font.css" rel="stylesheet" type="text/css">
        <link rel="stylesheet" type="text/css" href="style.css" >
    </head>
    <body>
        <div id="TOP">
            <div class="inside" id="channel"></div>
            <div class="inside" id="topic"></div>
            <div class="inside" id="username" contenteditable="true"
                autocapitalize="off" spellcheck="false"></div>
        </div>
        <div id="MIDDLE">
            <div id="chatroom"></div>
            <div id="drag"></div>
            <div id="userlist"></div>
        </div>
        <div id="BOTTOM">
            <input id="message" type="text" />
        </div>
        <script src="chat.js"></script>
    </body>
</html>
